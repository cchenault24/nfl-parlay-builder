# Mock Configuration Options
# Control mock behavior for testing different scenarios
VITE_MOCK_ERROR_RATE=0.05  # 5% chance of mock API errors
VITE_MOCK_DELAY_MIN=1000   # Minimum mock delay (ms)
VITE_MOCK_DELAY_MAX=2500   # Maximum mock delay (ms)
VITE_USE_MOCK_OPENAI=false  # Use mock OpenAI API (true/false)

# Rate Limiting Configuration
VITE_RATE_LIMIT_ENABLED=true
VITE_RATE_LIMIT_MAX_REQUESTS=10
